<!--pages/modify_tel/modify.wxml-->
<view class="weui-cells__title">联系电话</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <input class="weui-input" value='{{tel}}' bindinput='inputvalue' />
    </view>
  </view>
</view>
<view class='right'>
  <button class="weui-btn mini-btn" type="primary" size="mini" bindtap="post">完成</button>
</view>
